{
  "name": "zapbot",
  "version": "1.5.2",
  "description": "High-performance Solana copy trading bot with threading support",
  "main": "zapbot.js",
  "scripts": {
    "start": "node start.js",
    "start:legacy": "node zapbot.js",
    "start:threaded": "node start.js --threading",
    "dev": "nodemon start.js",
    "dev:legacy": "nodemon zapbot.js",
    "dev:threaded": "nodemon start.js --threading",
    "test": "jest",
    "test:threading": "jest tests/threading/",
    "test:coverage": "jest --coverage",
    "debug": "node scripts/debug.js",
    "setup:ec2": "bash deployment/ec2-setup.sh",
    "deploy": "bash /opt/zapbot/deploy.sh",
    "migrate": "node database/migrate.js",
    "backup": "bash /opt/zapbot/backup.sh",
    "monitor": "bash /opt/zapbot/monitor.sh",
    "health": "curl http://localhost:3001/health",
    "cleanup": "bash scripts/cleanup-ports.sh",
    "stop": "bash scripts/cleanup-ports.sh"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@jup-ag/api": "^6.0.0",
    "@meteora-ag/cp-amm-sdk": "^1.0.0",
    "@meteora-ag/dlmm": "^1.0.0",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.0.0",
    "@pump-fun/pump-sdk": "^1.3.4",
    "@raydium-io/raydium-sdk": "1.3.1-beta.5",
    "@solana-tracker/data-api": "^0.0.12",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.78.0",
    "axios": "^1.4.0",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "helius-laserstream": "^0.1.6",
    "jito-ts": "^1.1.0",
    "node-telegram-bot-api": "^0.61.0",
    "redis": "^4.6.7",
    "sqlite3": "^5.1.6",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "solana",
    "trading",
    "bot",
    "copy-trading",
    "pump.fun",
    "raydium",
    "meteora",
    "threading",
    "high-performance"
  ],
  "author": "ZapBot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/zapbot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/zapbot/issues"
  },
  "homepage": "https://github.com/your-username/zapbot#readme"
}
